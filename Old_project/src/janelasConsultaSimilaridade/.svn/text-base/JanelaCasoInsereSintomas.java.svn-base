    public JanelaCasoInsereSintomas(Caso caso, Paciente paciente) {
        this.setName("Escolha de Sintomas");

        this.caso = caso;
        this.paciente = paciente;
        initComponents();
        if (ReferenciasSimilaridade.getSintomasNaLista() != null) {
            this.sintomasNaLista = ReferenciasSimilaridade.getSintomasNaLista();
            colocaSintomasNaLista(sintomasNaLista);
        } else {
            this.sintomasNaLista = new ArrayList<String>();
        }
        labelPaciente.setText("diagnoscando: " + paciente.getNome());
        SintomaDAO dao = new SintomaDAO();
        ArrayList<Sintoma> lista = dao.selectNomeParcial(jTextField1.getText());
        atualizaLista(lista);

    }

