    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        // Adicionar >

        String[] adc = list1.getSelectedItems();

        for (String s : adc) {
            list2.add(s);
            list1.remove(s);
            sintomasNaLista.add(s);
        }


    }//GEN-LAST:event_jButton3ActionPerformed
    private void colocaSintomasNaLista(ArrayList<String> sintomas) {
        for (String sintoma : sintomas) {
            list2.add(sintoma);
        }
    }
    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        // < Remover

        String[] rem = list2.getSelectedItems();

        for (String r : rem) {
            list1.add(r);
            list2.remove(r);
            sintomasNaLista.remove(r);
        }

    }//GEN-LAST:event_jButton4ActionPerformed
    public void atualizaLista(ArrayList<Sintoma> sintomas) {
        list1.removeAll();
        Iterator iterator = sintomas.iterator();
        while (iterator.hasNext()) {
            Sintoma sintoma = (Sintoma) iterator.next();
            String nomeSintoma = sintoma.getNome();
            if (!sintomasNaLista.contains(nomeSintoma)) {
                list1.add(nomeSintoma);
            }

        }

    }

