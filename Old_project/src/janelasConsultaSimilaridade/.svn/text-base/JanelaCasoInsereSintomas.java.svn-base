    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
        ReferenciasSimilaridade.setSintomasNaLista(sintomasNaLista);

        String[] sintomas = list2.getItems();
        ArrayList<SintomaPeso> sintomasCaso = new ArrayList<SintomaPeso>();
        SintomaDAO dao = new SintomaDAO();
        for (String s : sintomas) {
            Sintoma sintoma = dao.selectNome(s);
            SintomaPeso sintomaPeso = new SintomaPeso(sintoma, null, 0);
            sintomasCaso.add(sintomaPeso);
        }
        this.caso.setSintomas(sintomasCaso);
        JanelaEscolheDoenca janela = new JanelaEscolheDoenca(caso, paciente);
        janela.setName("ConfirmaÃ§Ã£o do Diagnostico");
        JanelaPrincipal.getInstance().jTabbedPaneHome.add(janela);
        JanelaPrincipal.getInstance().jTabbedPaneHome.remove(this);

    }//GEN-LAST:event_jButton6ActionPerformed

