    public String getSexo() {
        String sexo = null;
        if (jRadioButton4.isSelected()) {
            sexo = "feminino";
        } else {
            if (jRadioButton5.isSelected()) {
                sexo = "masculino";
            } else {
                System.out.print("nenhum dos dois sexos foi selecionado");
            }
        }
        return sexo;
    }

    public Date getDataNasc() {

        try {
            String data = jFormattedTextField5.getText();
            int ano = Integer.parseInt(data.split("/")[2]);
            int dia = Integer.parseInt(data.split("/")[0]);
            int mes = Integer.parseInt(data.split("/")[1]);
            Date dataRetorno = new Date(ano, mes, dia);
            return dataRetorno;

        } catch (Exception e) {
            System.out.println("Erro ao converter data.");
            return null;
        }
    }

    public String getTipoSanguineo() {
        return (String) jComboBox2.getSelectedItem();
    }

    public int getTel() {
        String tel = jFormattedTextField3.getText();
        tel = tel.replaceAll("[(]", "");
        tel = tel.replaceAll("[)]", "");
        tel = tel.replaceAll("-", "");
        tel = tel.trim();
        tel = tel.substring(0, 1)+tel.substring(3,10);
        return Integer.parseInt(tel);
    }

    public int getCel() {
        String cel = jFormattedTextField4.getText();
        cel = cel.replaceAll("[(]", "");
        cel = cel.replaceAll("[)]", "");
        cel = cel.replaceAll("-", "");
        cel = cel.trim();
        cel = cel.substring(0, 1)+cel.substring(3,10);
        return Integer.parseInt(cel);
    }

