    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed

        nome = this.getNome();
        rg = this.getRG();
        cpf = this.getCPF();
        nasc = this.getDataNasc();
        sexo = this.getSexo();
        tipoSang = this.getTipoSanguineo();
        endereco = this.getEndereco();
        endComplemento = this.getEndComplemento();
        cidade = this.getCidade();
        estado = this.getEstado();
        telRes = this.getTel();
        telCel = this.getCel();
        estCivil = this.getEstadoCivil();

        JOptionPane frame = new JOptionPane();

        if (Controle.Validacao.verificaRG(rg.toString())) {
            FichaMedica fm = new FichaMedica(0, sexo, tipoSang);

            Paciente pac = new Paciente(0, cpf, telRes, telCel, nome,
                    rg, endereco, endComplemento, cidade,
                    estado, estCivil, nasc, fm);
            FichaMedicaDAO fmdao = new FichaMedicaDAO();
            fmdao.incluir(fm);
            PacienteDAO pdao = new PacienteDAO();
            pdao.incluir(pac);

            //INSERIR LIGAÇÃO LOCAL - MÉDICO
            Medico med = Referencias.getMedicoLogado();
            int codMed = med.getCodigo();

            PacienteDAO pacDAO = new PacienteDAO();
            int codPac = pacDAO.selectUltimoCod();

            AgendaDAO ageDAO = new AgendaDAO();
            ageDAO.relacaoMedicoPacicente(codMed, codPac);

            JanelaPrincipal.getInstance().jTabbedPaneHome.remove(this);

            JOptionPane.showMessageDialog(frame,
                    "Os dados foram cadastrados com sucesso.",
                    "Confirmação",
                    JOptionPane.PLAIN_MESSAGE);
        } else {
            JOptionPane.showMessageDialog(frame,
                    "Favor inserir RG válido.",
                    "ATENÇÃO",
                    JOptionPane.WARNING_MESSAGE);
        }
