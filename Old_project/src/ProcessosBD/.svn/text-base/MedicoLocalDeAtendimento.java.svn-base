package ProcessosBD;

/**
 *
 * @author Glaupe e Iris
 */
public class MedicoLocalDeAtendimento {
    private int codigo_med;
    private int codigo_consultorio;

    public MedicoLocalDeAtendimento() {
    }

    public MedicoLocalDeAtendimento(int codigo_med, int codigo_consultorio) {
        this.codigo_med = codigo_med;
        this.codigo_consultorio = codigo_consultorio;
    }

    public int getCodigo_consultorio() {
        return codigo_consultorio;
    }

    public void setCodigo_consultorio(int codigo_consultorio) {
        this.codigo_consultorio = codigo_consultorio;
    }
    
    public int getCodigo_medico() {
        return codigo_med;
    }

    public void setCodigo_medico(int codigo_medico) {
        this.codigo_med = codigo_medico;
    }
    
}
