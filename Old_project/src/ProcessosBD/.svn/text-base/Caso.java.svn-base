package ProcessosBD;

import java.util.ArrayList;

public class Caso {
    private int codigo;
    private String nome;
    private String medicacoes;
    private float altura;
    private int idade;
    private float peso;
    private Doenca doenca;
    private Medico medico;
    private ArrayList<SintomaPeso> sintomas;
    
    public Caso(){        
    }

    public Caso(int codigo, String nome, String medicacoes, float altura, int idade, float peso, Doenca doenca, Medico medico, ArrayList<SintomaPeso> sintomas) {
        this.codigo = codigo;
        this.nome = nome;
        this.medicacoes = medicacoes;
        this.altura = altura;
        this.idade = idade;
        this.peso = peso;
        this.doenca = doenca;
        this.medico = medico;
        this.sintomas = sintomas;
    }

    //<editor-fold defaultstate="collapsed" desc="gets/sets">
    public float getAltura() {
        return altura;
    }

    public void setAltura(float altura) {
        this.altura = altura;
    }

    public int getCodigo() {
        return codigo;
    }

    public void setCodigo(int codigo) {
        this.codigo = codigo;
    }

    public Doenca getDoenca() {
        return doenca;
    }

    public void setDoenca(Doenca doenca) {
        this.doenca = doenca;
    }

    public int getIdade() {
        return idade;
    }

    public void setIdade(int idade) {
        this.idade = idade;
    }

    public String getMedicacoes() {
        return medicacoes;
    }

    public void setMedicacoes(String medicacoes) {
        this.medicacoes = medicacoes;
    }

    public Medico getMedico() {
        return medico;
    }

    public void setMedico(Medico medico) {
        this.medico = medico;
    }

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public float getPeso() {
        return peso;
    }

    public void setPeso(float peso) {
        this.peso = peso;
    }

    public ArrayList<SintomaPeso> getSintomas() {
        return sintomas;
    }

    public void setSintomas(ArrayList<SintomaPeso> sintomas) {
        this.sintomas = sintomas;
    }
    //</editor-fold>   

    
       
}
