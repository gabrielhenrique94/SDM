package ProcessosBD;

public class UsuarioNovaDoenca {
    
    private NovaDoenca novaDoenca;
    private Usuario usuario;
    private int votou;

    public UsuarioNovaDoenca() {
    }

    public UsuarioNovaDoenca(NovaDoenca novaDoenca, Usuario usuario, int votou) {
        this.novaDoenca = novaDoenca;
        this.usuario = usuario;
        this.votou = votou;
    }

    public NovaDoenca getNovaDoenca() {
        return novaDoenca;
    }

    public void setNovaDoenca(NovaDoenca novaDoenca) {
        this.novaDoenca = novaDoenca;
    }

    public Usuario getUsuario() {
        return usuario;
    }

    public void setUsuario(Usuario usuario) {
        this.usuario = usuario;
    }

    public int getVotou() {
        return votou;
    }

    public void setVotou(int votou) {
        this.votou = votou;
    }
    
}
